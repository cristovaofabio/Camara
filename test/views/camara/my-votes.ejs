<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <main>
      <% if(projects.length>0){ %>
      <ul>
        <% projects.forEach(p => { %>
        <li><%=p.projectData.title%> (<%=p.typeVote%>)</li>
        <% }) %>
      </ul>
      <% } else { %>
      <h1>Você ainda não votou!</h1>
      <% } %>
    </main>
  </body>
</html>
